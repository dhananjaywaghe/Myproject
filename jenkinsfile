pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                // Clone the GitHub repository
                git branch: 'main', url: 'https://github.com/dhananjaywaghe/Myproject.git'
            }
        }

        stage('Execute Python Script') {
            steps {
                // Execute the hello.py script
                sh 'python3 hello.py'
            }
        }
    }
}
